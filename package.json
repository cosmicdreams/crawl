  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:storybook": "test-storybook",
    "test:all": "npm run test && npm run test:storybook",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=./test-results/junit.xml",
    "test:unit": "vitest run --coverage --environment=node 'src/**/*.test.ts'",
    "test:components": "vitest run --coverage --environment=jsdom 'src/**/*.test.tsx'",
    "build": "tsc && node src/api/copy-public.js",
    "start": "concurrently \"npm run start:api\" \"npm run start:ui\"",
    "crawl": "npm run build && node dist/cli/run.js crawl",
    "crawl:pncb": "node dist/cli/run.js crawl --profile pncb",
    "crawl:example": "npm run build && node dist/cli/run.js crawl --url https://example.com --max-pages 5",
    "extract:colors": "npm run build && node dist/cli/run.js crawl --extractors colors",
    "extract:typography": "npm run build && node dist/cli/run.js crawl --extractors typography",
    "extract:spacing": "npm run build && node dist/cli/run.js crawl --extractors spacing",
    "extract:borders": "npm run build && node dist/cli/run.js crawl --extractors borders",
    "extract:all": "npm run build && node dist/cli/run.js crawl --extractors all",
    "generate-tokens": "npm run build && node dist/cli/run.js crawl --extractors all --generate-tokens",
    "start:api": "npm run build && node dist/api/start-server.js",
    "start:ui": "storybook dev -p 6006",
    "dev:api": "tsc && concurrently \"tsc -w\" \"nodemon dist/api/server.js\"",
    "dev:ui": "vite",
    "dev": "concurrently \"npm run dev:ui\" \"npm run dev:api\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "lint": "eslint . --ext .js,.ts,.tsx"
  },
