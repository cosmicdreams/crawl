{
  "name": "site-crawler",
  "version": "1.0.0",
  "description": "A phased approach to site crawling and analysis tool",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node --disable-warning=DEP0169 index.js",
    "initial": "node --disable-warning=DEP0169 index.js initial",
    "deepen": "node --disable-warning=DEP0169 index.js deepen",
    "metadata": "node --disable-warning=DEP0169 index.js metadata",
    "organize": "node --disable-warning=DEP0169 index.js organize",
    "extract": "node --disable-warning=DEP0169 index.js extract",
    "all": "node --disable-warning=DEP0169 index.js all",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run src/**/*.test.{js,ts}",
    "test:integration": "vitest run tests/integration/**/*.test.{js,ts}",
    "test:edge-cases": "vitest run tests/edge-cases/**/*.test.{js,ts}",
    "test:performance": "vitest run tests/integration/performance.test.ts",
    "test:security": "pnpm audit && vitest run tests/security/**/*.test.{js,ts}",
    "test:cli": "vitest run tests/console/**/*.test.{js,ts}"
  },
  "keywords": [
    "crawler",
    "site",
    "analysis",
    "css",
    "styleguide"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@playwright/test": "^1.55.0",
    "@types/uuid": "^10.0.0",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "jsdom": "^26.1.0",
    "ora": "^8.2.0",
    "reactflow": "^11.11.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@storybook/addon-docs": "9.1.3",
    "@storybook/addon-vitest": "9.1.3",
    "@storybook/react-vite": "9.1.3",
    "@storybook/test": "9.0.0-alpha.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-storybook": "9.1.3",
    "playwright": "^1.55.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "storybook": "9.1.3",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  }
}
