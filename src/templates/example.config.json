{
  "baseUrl": "http://localhost:8080",
  "outputDir": "./results",
  "maxPages": 100,
  "timeout": 30000,
  "screenshotsEnabled": true,
  "ignoreExtensions": [
    ".pdf",
    ".jpg",
    ".jpeg",
    ".png",
    ".gif",
    ".svg",
    ".css",
    ".js",
    ".zip",
    ".tar",
    ".gz"
  ],
  "ignorePatterns": [
    "\\?",
    "/admin/",
    "/user/",
    "/cart/",
    "/checkout/",
    "/search/"
  ],
  "respectRobotsTxt": true,
  "twigDebugMode": false,
  "FilteredComponentList": [
    "button",
    "card",
    "modal",
    "navigation",
    "banner"
  ],
  "telemetry": {
    "enabled": true,
    "outputDir": "./results/telemetry",
    "logToConsole": true,
    "writeToFile": true,
    "minDuration": 5,
    "includeTimestamps": true,
    "includeMemoryUsage": true
  },
  "logging": {
    "debug": true,
    "logLevel": "INFO",
    "colors": true,
    "timestamps": true,
    "prefix": "crawler"
  }
}
